      "source": [
        "def fibonacci_generator():\n",
        "    a, b = 0, 1\n",
        "    while True:\n",
        "        yield a\n",
        "        a, b = b, a + b\n",
        "\n",
        "# Example usage:\n",
        "n = 10\n",
        "fib_gen = fibonacci_generator()\n",
        "fibonacci_list = [next(fib_gen) for _ in range(n)]\n",
        "print(fibonacci_list)"
      ]
    }
